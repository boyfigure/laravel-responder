{
    "name": "offspring/laravel-responder",
    "description": "A Laravel Fractal package for building API responses, giving you the power of Fractal and the elegancy of Laravel.",
    "keywords": [
        "laravel",
        "api",
        "response",
        "responder"
    ],
    "homepage": "https://github.com/boyfigure/laravel-responder",
    "license": "MIT",
    "authors": [
        {
            "name": "tony",
            "email": "tony@offspringdigital.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/contracts": "5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.* || 5.6.* || 5.7.* || 5.8.* || ^6.0.0",
        "illuminate/support": "5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.* || 5.6.* || 5.7.* || 5.8.* || ^6.0.0",
    },
    "autoload": {
        "psr-4": {
            "Flugg\\Responder\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Flugg\\Responder\\ResponderServiceProvider"
            ],
            "aliases": {
                "Responder": "Flugg\\Responder\\Facades\\Responder"
            }
        }
    }
}
